.pswp--touch .pswp__button--arrow--right {
  visibility: hidden;
}

/*
Arrow buttons hit area
(icon is added to :before pseudo-element)
*/
.pswp__button--arrow--left,
.pswp__button--arrow--right {
  background: none;
  top: 50%;
  margin-top: -22px;
  width: 30px;
  height: 32px;
  position: absolute;
}

